<!DOCTYPE html>
<html lang="{{site.locale}}" {{#if site.rtl}}dir="rtl"{{/if}}>
{{> html-head }}

<body>
  {{> header }}

  <div id="main">
    <div class="container">

      <div id="home-intro">

        {{{ contents }}}

        <p>
          <a class="imagelink home-banner" href="https://events.linuxfoundation.org/events/js-interactive-2018/?utm_source=omarsite&utm_medium=homepagebanner&utm_campaign=jsint18&utm_term=eventpage">
            <img src="/static/images/jsinteractive-2018-banner.png" alt="JS Interactive on October 10-12, 2018 in Vancouver, Canada">
          </a>
        </p>

        {{#if project.banner.visible}}
          <p class="home-version home-version-banner">
            <a href="{{ project.banner.link }}">{{#if project.banner.text}}{{{ project.banner.text }}}{{else}}{{{ labels.banner }}}{{/if}}</a>
          </p>
        {{/if}}

        <h2 id="home-downloadhead" data-dl-local="{{ labels.download-for }}">{{ labels.download }}</h2>

        <div class="home-downloadblock">

          <a href="https://omarjs.org/dist/{{ project.latestVersions.lts.omar }}/" class="home-downloadbutton" title="{{ labels.download }} {{stripv project.latestVersions.lts.omar }} {{ labels.lts }}" data-version="{{ project.latestVersions.lts.omar }}">
            {{stripv project.latestVersions.lts.omar }} {{ labels.lts }}
            <small>{{ labels.tagline-lts }}</small>
          </a>

          <ul class="list-divider-pipe home-secondary-links">
            <li>
              <a href="https://omarjs.org/{{site.locale}}/download/">{{ labels.other-downloads }}</a>
            </li>
            <li>
              <a href="{{changeloglink project.latestVersions.lts.omar}}">{{ labels.changelog }}</a>
            </li>
            <li>
              <a href="{{majorapidocslink project.latestVersions.lts.omar}}">{{ labels.api }}</a>
            </li>
          </ul>

        </div>

        {{#if project.latestVersions.current.omar}}
          <div class="home-downloadblock">

            <a href="https://omarjs.org/dist/{{ project.latestVersions.current.omar }}/" class="home-downloadbutton"  title="{{ labels.download }} {{stripv project.latestVersions.current.omar }} {{ labels.current }}"  data-version="{{ project.latestVersions.current.omar }}">
              {{stripv project.latestVersions.current.omar }} {{ labels.current }}
              <small>{{ labels.tagline-current }}</small>
            </a>

            <ul class="list-divider-pipe home-secondary-links">
              <li>
                <a href="https://omarjs.org/{{site.locale}}/download/current/">{{ labels.other-downloads }}</a>
              </li>
              <li>
                <a href="{{changeloglink project.latestVersions.current.omar}}">{{ labels.changelog }}</a>
              </li>
              <li>
                <a href="{{majorapidocslink project.latestVersions.current.omar}}">{{ labels.api }}</a>
              </li>
            </ul>

          </div>
        {{/if}}
        <p>
          {{{ labels.version-schedule-prompt }}} <a href="https://github.com/omarjs/Release#release-schedule">{{ labels.version-schedule-prompt-link-text }}</a>
        </p>
        {{#if labels.newsletter }}
        <p>
          {{{ labels.newsletter-prefix }}} <a href="https://newsletter.omarjs.org/">Node.js Everywhere</a>{{{ labels.newsletter-postfix }}}
        </p>
        {{/if}}
      </div>

    </div>
  </div>

  {{> footer className="no-margin-top" }}
  <script src="/static/js/download.js" async defer></script>

</body>
</html>
